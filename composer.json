{
    "name": "slope-it/breadcrumb-bundle",
    "description": "A bundle for generating dynamic breadcrumbs in Symfony applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "Breadcrumb",
        "Breadcrumbs",
        "Symfony",
        "Bundle",
        "slope",
        "slope.it"
    ],
    "authors": [
        {
            "name": "Andrea Sprega",
            "email": "andrea.sprega@slope.it",
            "role": "Software engineer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "doctrine/annotations": "^1.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/property-access": "^4.0|^5.0",
        "symfony/translation-contracts": "^1.0|^2.0",
        "twig/twig": "^2.10|^3.0"
    },
    "require-dev": {
        "mockery/mockery": "1.3.4",
        "phpunit/phpunit": "8.5.15"
    },
    "autoload": {
        "psr-4": {
            "SlopeIt\\BreadcrumbBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlopeIt\\Tests\\BreadcrumbBundle\\": "tests"
        }
    },
    "archive": {
        "exclude": [
            ".github",
            "/tests"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
